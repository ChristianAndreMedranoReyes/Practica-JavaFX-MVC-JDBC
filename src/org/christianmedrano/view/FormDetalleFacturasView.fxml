<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="750.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="org.christianmedrano.controller.FormDetalleFacturasController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="500.0">
         <image>
            <Image url="@../images/ImagenFormDetalleFacturas.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfDetalleFacturaId" layoutX="76.0" layoutY="211.0" prefHeight="33.0" prefWidth="279.0" style="-fx-background-color: #5ce1e6; -fx-border-color: black;" />
      <ComboBox fx:id="cmbFacturas" layoutX="76.0" layoutY="300.0" prefHeight="33.0" prefWidth="279.0" style="-fx-background-color: #5ce1e6; -fx-border-color: black;" />
      <ComboBox fx:id="cmbProductos" layoutX="76.0" layoutY="389.0" prefHeight="33.0" prefWidth="279.0" style="-fx-background-color: #5ce1e6; -fx-border-color: black;" />
      <Button fx:id="btnGuardar" layoutX="69.0" layoutY="703.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="108.0" style="-fx-background-radius: 50; -fx-background-color: #5ce1e6; -fx-border-color: black; -fx-border-radius: 50;" text="Guardar" />
      <Button fx:id="btnRegresar" layoutX="200.0" layoutY="703.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="108.0" style="-fx-background-radius: 50; -fx-background-color: #5ce1e6; -fx-border-color: black; -fx-border-radius: 50;" text="Regresar" />
   </children>
</AnchorPane>
